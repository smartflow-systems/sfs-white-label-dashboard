# SFS White Label Dashboard - Environment Configuration

# Application
NODE_ENV=development
PORT=5012
VITE_API_URL=http://localhost:5012

# Database
DATABASE_URL=postgresql://user:password@host/database?sslmode=require

# Multi-Tenancy
ENABLE_MULTI_TENANCY=true
DEFAULT_TENANT=demo

# Branding & Customization
ENABLE_CUSTOM_BRANDING=true
ENABLE_CUSTOM_DOMAIN=true
LOGO_STORAGE_PATH=/uploads/logos

# File Storage
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_S3_BUCKET=sfs-white-label-assets
AWS_REGION=us-east-1

# DNS Management (for custom domains)
CLOUDFLARE_API_TOKEN=your-cloudflare-token
CLOUDFLARE_ZONE_ID=your-zone-id

# Theme Configuration
DEFAULT_PRIMARY_COLOR=#3b82f6
DEFAULT_THEME=light
ALLOW_DARK_MODE=true

# Feature Flags (per tenant)
ENABLE_ANALYTICS=true
ENABLE_REPORTING=true
ENABLE_API_ACCESS=true

# Authentication
JWT_SECRET=your-jwt-secret-key
SESSION_SECRET=your-session-secret

# Billing Integration
STRIPE_SECRET_KEY=sk_test_your_stripe_key
STRIPE_PUBLIC_KEY=pk_test_your_stripe_public_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret
ENABLE_USAGE_TRACKING=true

# Stripe Product/Price IDs (create in Stripe Dashboard)
STRIPE_PRO_PRICE_ID=price_pro_monthly_id
STRIPE_PRO_ANNUAL_PRICE_ID=price_pro_annual_id
STRIPE_ENTERPRISE_PRICE_ID=price_enterprise_monthly_id
STRIPE_ENTERPRISE_ANNUAL_PRICE_ID=price_enterprise_annual_id

# Stripe Success/Cancel URLs
STRIPE_SUCCESS_URL=http://localhost:5000/billing?success=true
STRIPE_CANCEL_URL=http://localhost:5000/pricing?canceled=true

# Referral Program
ENABLE_REFERRALS=true
REFERRAL_COMMISSION_RATE=0.30
REFERRAL_COOKIE_DAYS=30

# Usage Limits (Free Tier)
FREE_TIER_API_CALLS=1000
FREE_TIER_STORAGE_MB=100
FREE_TIER_DATA_RETENTION_DAYS=7

# Usage Limits (Pro Tier)
PRO_TIER_API_CALLS=100000
PRO_TIER_STORAGE_GB=10
PRO_TIER_DATA_RETENTION_DAYS=90

# Usage Limits (Enterprise Tier)
ENTERPRISE_TIER_API_CALLS=999999999
ENTERPRISE_TIER_STORAGE_GB=1000
ENTERPRISE_TIER_DATA_RETENTION_DAYS=365

# ------------------------------------------------------------------------------
# SMARTFLOW SYSTEMS COMMON SECRETS
# ------------------------------------------------------------------------------
# These secrets are shared across all SFS applications

# Client URL(s) for CORS (comma-separated for multiple origins)
# Development: http://localhost:3000
# Production: https://yourdomain.replit.app
CLIENT_URL=http://localhost:3000

# Alternative CORS origin configuration
# Use when you need different CORS settings than CLIENT_URL
CORS_ORIGIN=http://localhost:3000

# SmartFlow Systems Personal Access Token
# Used for inter-service authentication and GitHub API access
# Generate at: https://github.com/settings/tokens
# IMPORTANT: Replace with your actual token - do NOT commit real tokens
SFS_PAT=

# JWT Secret for authentication tokens
# Generate with: openssl rand -base64 32
JWT_SECRET=your-secret-key-change-in-production

# API Sync Token for webhook authentication
# Generate with: openssl rand -hex 32
SYNC_TOKEN=your-sync-token-change-in-production

# ------------------------------------------------------------------------------
# REPLIT DEPLOYMENT CONFIGURATION
# ------------------------------------------------------------------------------
# These are AUTO-SET by Replit - do not manually configure

# Replit ID (auto-set)
# REPL_ID=

# Replit owner username (auto-set)
# REPL_OWNER=

# Replit authentication token (auto-set, used for webhooks)
# REPLIT_TOKEN=

# Replit database URL (auto-set if using Replit DB)
# REPLIT_DB_URL=

# Frontend URL for Replit deployments
# Example: https://sfs-core-services.yourusername.replit.app
FRONTEND_URL=

# ------------------------------------------------------------------------------
# GATEWAY CONFIGURATION (if using microservices)
# ------------------------------------------------------------------------------
# API Gateway port
GATEWAY_PORT=4000

# Microservice URLs (update with actual service URLs)
MARKETING_SERVICE_URL=http://localhost:3001
SOCIAL_SERVICE_URL=http://localhost:3002
SCRAPER_SERVICE_URL=http://localhost:3003
QUERY_SERVICE_URL=http://localhost:3004

